int main(void)
{
	int interger1 = 0;
	int interger2 = 0;
	int sum = 0;

	printf("enter frist interger\n");
	scanf_s("%d", &interger1);

	printf("enter second interger\n");
	scanf_s("%d", &interger2);

	sum = interger1 + interger2;
	printf("加 %d\n", sum);

	sum = interger1 - interger2;
	printf("減 %d\n", sum);

	sum = interger1 * interger2;
	printf("乘 %d\n", sum);

	sum = interger1 / interger2;
	printf("商 %d\n", sum);

	sum = interger1 % interger2;
	printf("餘數 %d\n", sum);
}
