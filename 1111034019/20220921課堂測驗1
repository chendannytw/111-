	int integer1 = 0;
	int integer2 = 0;
	int sum = 0;

	printf("enter frist integer\n");
	scanf_s("%d", &integer1);

	printf("enter second integer\n");
	scanf_s("%d", &integer2);

	sum = integer1 + integer2;
	printf("加為 %d\n", sum);

	sum = integer1 - integer2;
	printf("減為 %d\n", sum);

	sum = integer1 * integer2;
	printf("乘為 %d\n", sum);

	sum = integer1 / integer2;
	printf("商為 %d\n", sum);

	sum = integer1 % integer2;
	printf("餘數為 %d\n", sum);
