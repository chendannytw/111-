#include <stdio.h>
#define SIZE 5
void myfun(int arr[], int num);
// function main begins program execution
int main(void)
{
    // use initializer list to initialize array n
    int n[SIZE] = { 19, 3, 15, 7, 11 }, i, j;

    printf("%s%13s%17s\n", "Element ", "Value", "Histogram");
    myfun(n, SIZE);
        // for each element of array n, output a bar of the histogram
   }
void myfun(int arr[], int num)
{  
    int i, j;
    for (i = 0; i < num; ++i) {
    printf("%7u%13d   ", i, arr[i]);

    for (j = 1; j <= arr[i]; ++j) { // print one bar
        printf("%c", '*');
    }

    puts(""); // end a histogram bar with a newline
}
   

 }
 
 

#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#define RESPONSES_SIZE 40
#define FREQUENCY_SIZE 11
void KINGDOMCUM(int n[], int m[]);
int main(void)
{

    int frequency[FREQUENCY_SIZE] = { 0 };

    
    int responses[RESPONSES_SIZE] = { 1, 2, 6, 4, 8, 5, 9, 7, 8, 10,
         1, 6, 3, 8, 6, 10, 3, 8, 2, 7, 6, 5, 7, 6, 8, 6, 7, 5, 6, 6,
         5, 6, 7, 5, 6, 4, 8, 6, 8, 10 };
    KINGDOMCUM(frequency, responses);
}
void KINGDOMCUM(int n[], int m[]) {
    for (size_t answer = 0; answer < RESPONSES_SIZE; ++answer) {
        ++n[m[answer]];
    }
    printf_s("%s%17s%14s\n", "Rating", "Frequency","Chart");
    for (size_t rating = 1; rating < FREQUENCY_SIZE; ++rating) {
        printf_s("%6d%17d           ", rating, n[rating]);
        for (int star_num = 0; star_num < n[rating]; star_num++) {
            printf_s("%c",'*');
        }
        puts("");
    }
}
