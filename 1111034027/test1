#include <stdio.h>

int main(void)
{
    int integer1 = 0 ;
    int integer2 = 0;
    printf("Enter first integer:");
    scanf_s("%d", &integer1);
    printf("Enter first integer:");
    scanf_s("%d", &integer2);
    int sum = 0;
    int product = 0;
    int x = 0;
    int quo = 0;
    int mod = 0;
    sum = integer1 + integer2;
    printf("sum is %d\n",sum);
    product= integer1 - integer2;
    printf("product is %d\n", product);
    x= integer1 * integer2;
    printf("x is %d\n", x);
    quo = integer1 / integer2;
    printf("quo is %d\n", quo);
    mod = integer1 % integer2;
    printf("rem is %d\n", mod);
}
