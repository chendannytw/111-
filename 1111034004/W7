#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
unsigned long long int f (int n);
int main(void) {
int n;
printf("輸入n");
scanf("%d", &n);
{
	printf("F(%d)=%11u\n",n,f(n));
}

}
unsigned long long int f(n)
 {
 	if (n<=1)
	    return n;
	else
	     return f(n-1)+f(n-2);
}
