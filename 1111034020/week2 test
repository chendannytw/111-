int main(void)

    {
        int integer1 = 0;
        int integer2 = 0;
        printf("Enter first integer : ");
        scanf_s("%d", &integer1);

        printf("Enter second integer : ");
        scanf_s("%d", &integer2);

        int sum = 0;
        int reduce = 0;
        int chen = 0;
        int average = 0;
        int mod = 0;

        sum = integer1 + integer2;
        reduce = integer1 - integer2;
        chen = integer1 * integer2;
        average = integer1 / integer2;
        mod = integer1 % integer2;


        printf("Sum is %d\n", sum);
        printf("reduce is %d\n", reduce);
        printf("chen is %d\n", chen);
        printf("average is %d\n", average);
        printf("mod is %d\n", mod);
    }
   
