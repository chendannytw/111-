#include <stdio.h>

int main(void)
{
	int integer1 = 0, integer2 = 0, sum1 = 0, sum2 = 0, sum3 = 0, sum4 = 0, sum5 = 0;
	printf("Enter first integer:");
	scanf_s("%d", &integer1);
	printf("Enter second integer:");
	scanf_s("%d", &integer2);
	sum1 = integer1 + integer2;
	sum2 = integer1 - integer2;
	sum3 = integer1 * integer2;
	sum4 = integer1 / integer2;
	sum5 = integer1 % integer2;
	printf("和: %d\n", sum1);
	printf("差: %d\n", sum2);
	printf("乘積: %d\n", sum3);
	printf("商: %d\n", sum4);
	printf("餘數: %d\n", sum5);
}
