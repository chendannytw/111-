// ConsoleApplication1.cpp : 此檔案包含 'main' 函式。程式會於該處開始執行及結束執行。
//

#include <iostream>


/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(void) {
	int v, a, e, s;
	
	printf("物體速度\n");
	scanf_s("%d",&v);
	printf("加速度\n");
	scanf_s("%d",&a);
	printf("經過時間\n");
	scanf_s("%d",&e);
	
	v  = v +a * e;
	printf("最後速度 %d\n" ,v);
	s = v＊ｅ　+ 0.5 * a * e;
	printf("距離: %d\n" , s);
}
