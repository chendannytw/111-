// ConsoleApplication1.cpp : 此檔案包含 'main' 函式。程式會於該處開始執行及結束執行。
//

#include <iostream>


/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(void) {
	int integer1 = 0;
	int integer2 = 0;
	int integer3 = 0;
	printf("物體速度:");
	scanf_s("%d", &integer1);

	printf("加速度:");
	scanf_s("%d", &integer2);

	printf("經過時間:");
	scanf_s("%d", &integer3);

	int sum = 0;
	sum = integer1 + integer2 ;
	sum = sum  *integer3;
	printf("最後速度:")& sum;


	int sum1 = 0;
	sum1 = integer2 * integer3 + (1 / 2) * integer2 * integer3 * integer3;
	printf("移動速度:") & sum1;
	

}
