#include <stdio.h>
#include <string.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int c,i=0;
	char s1[13]="abCDabCDabCD";
	char s2[13]="CD";
	for (i=0;i<12;i++){
	    if (strncmp(s2,s1+i,2)==0)
	        c+=1;		
	}
	printf("CD 在 abCDabCDabCD 出現 %d 次",c );
	
}


//聖誕快樂
#include <stdio.h>
#include <string.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int c,i=0;
	char s1[13]="abCDabCDabCD";
	char s2[13]="CD";
	for (i=0;i<12;i++){
	    if (strncmp(s2,s1+i,2)==0)
	        c+=1;		
	}
	printf("%s 在 %s 出現 %d 次",s2,s1,c );
	
}
