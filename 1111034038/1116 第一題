#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define cumonslut 5
void hugecum(int ass[], int num);

int main(void) {
	int n[cumonslut] = {19,3,15,7,11};
	printf_s("%s%13s%17s\n", "Element", "value", "bar chart");
	hugecum (n,cumonslut);
}

void hugecum(int ass[],int num) 
{
	long long int i,j;
	for (i = 0; i < num; ++i) {
		printf_s( "%7zu%13d%8s", i, ass[i], "");
		for (int j = 1; j <= ass[i]; ++j) {
			printf_s("%c", '*');
		}
		puts("");

	}
}
