#include <stdio.h>
#include <stdlib.h>




int main(void)
{unsigned int i,n;
unsigned int result,temp,temp1;

printf("%s","Enter n:");
scanf("%u",&n);

srand(n);
temp=pow(10,n);

for(i=1;i<=temp;++i){
	temp1=(rand()%temp);
	printf("%131f",(double)temp1/pow(10,n));
	
	if(i%5==0){
		puts("");
	}
}


}
